{
	"name" : "internetrix/silverstripe-versionedextensions",
	"description" : "Extension for versioned has_many and many_many relationship.",
	"type" : "silverstripe-vendormodule",
	"homepage": "https://gitlab.internetrix.net/silverstripe-modules/template",
	"keywords" : [
		"silverstripe",
		"versioned",
		"has_many",
		"many_many",
		"versioning",
		"extension"
	],
	"license": "BSD-3-Clause",
	"authors" : [{
			"name" : "Guy Watson",
			"email" : "guy.watson@internetrix.com.au"
		}, {
			"name" : "Jason Zhang",
			"email" : "jason.zhang@internetrix.com.au"
		}
	],
	"support": {
		"issues": "https://gitlab.internetrix.net/silverstripe-modules/template/issues"
	},

	"require" : {
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/framework" : "~4.0",
        "silverstripe/cms" : "~4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^3.0"
	},
	"extra" : {
		"installer-name" : "versionedextensions"
	},
	"autoload": {
		"psr-4": {
			"Internetrix\\VersionedExtensions\\": "src/"
		}
	},
    "prefer-stable": true,
	"minimum-stability" : "dev"
}